{
  "name": "@velmoo/types",
  "version": "1.0.0",
  "description": "Collection of utility types.",
  "scripts": {
    "build": "pnpm format && tsup && pnpm gen-pkg-exports && pnpm del-js-files",
    "dev": "tsup --watch",
    "lint": "tsc",
    "format": "prettier --write **/*.{js,ts,json}",
    "gen-pkg-exports": "ts-node -r tsconfig-paths/register scripts/gen-pkg-exports.ts",
    "del-js-files": "ts-node -r tsconfig-paths/register scripts/del-js-files.ts"
  },
  "author": "Rabah Taib",
  "repository": {
    "type": "git",
    "url": "https://github.com/velmoo/types"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json",
    "./array": "./dist/array.js",
    "./common": "./dist/common.js",
    "./expensive": "./dist/expensive/index.js",
    "./expensive/*": "./dist/expensive/*.js",
    "./index": "./dist/index.js",
    "./mix": "./dist/mix.js",
    "./number": "./dist/number.js",
    "./object": "./dist/object.js",
    "./string": "./dist/string.js"
  },
  "keywords": [
    "velmo",
    "utils",
    "typescript",
    "types"
  ],
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.7.4",
    "@types/vscode": "^1.94.0",
    "fs-extra": "^11.2.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2"
  }
}
